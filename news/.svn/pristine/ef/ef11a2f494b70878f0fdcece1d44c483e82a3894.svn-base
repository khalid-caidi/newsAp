package ma.marsamaroc.entite;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="DONNEEDECONF")
public class Donneedeconf implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@Column(name="DONNEEDECONF")
	private String donneeDeConf;
	
	@Column(name="VALEUR_INITIALE_DONNEEDECONF")
	private String valeurInitialeDonneeDeConf;
	
	@Column(name="VALEUR_ACTUELLE_DONNEEDECONF")
	private String valeurActuelleDonneeDeConf;
	
	@Column(name="DATE_MAJ_DONNEEDECONF")
	private String dateMAJDonneeDeConf;
	
	@Column(name="DESCRIPTION_DONNEEDECONF")
	private String descriptionDonneeDeConf;

	public String getDonneeDeConf() {
		return donneeDeConf;
	}

	public void setDonneeDeConf(String donneeDeConf) {
		this.donneeDeConf = donneeDeConf;
	}

	public String getValeurInitialeDonneeDeConf() {
		return valeurInitialeDonneeDeConf;
	}

	public void setValeurInitialeDonneeDeConf(String valeurInitialeDonneeDeConf) {
		this.valeurInitialeDonneeDeConf = valeurInitialeDonneeDeConf;
	}

	public String getValeurActuelleDonneeDeConf() {
		return valeurActuelleDonneeDeConf;
	}

	public void setValeurActuelleDonneeDeConf(String valeurActuelleDonneeDeConf) {
		this.valeurActuelleDonneeDeConf = valeurActuelleDonneeDeConf;
	}

	public String getDateMAJDonneeDeConf() {
		return dateMAJDonneeDeConf;
	}

	public void setDateMAJDonneeDeConf(String dateMAJDonneeDeConf) {
		this.dateMAJDonneeDeConf = dateMAJDonneeDeConf;
	}

	public String getDescriptionDonneeDeConf() {
		return descriptionDonneeDeConf;
	}

	public void setDescriptionDonneeDeConf(String descriptionDonneeDeConf) {
		this.descriptionDonneeDeConf = descriptionDonneeDeConf;
	}

}
